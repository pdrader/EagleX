DELIMITER //

CREATE PROCEDURE InsertToDeadhead(
	IN pProNumber VARCHAR(50),
	IN pRPM DECIMAL(10,2),
	IN pFlat DECIMAL(10,2),
	IN pMiles INT,
	IN pFileName VARCHAR(255),
	IN pSubject VARCHAR(255)

)
BEGIN
	REPLACE INTO deadhead (ProNumber, RPM, Flat, Miles, FileName, Subject)
	VALUES (pProNumber, pRPM, pFlat, pMiles, pFileName, pSubject);	
END //

DELIMITER ;